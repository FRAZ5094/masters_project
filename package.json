{
  "name": "masters_project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && mv dist docs",
    "preview": "vite preview",
    "test": "jest --watch",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve .",
    "testas": "watchexec -w assembly -w tests 'npm run asbuild:debug; node tests/index.js'",
    "benchmark": "ts-node ./src/benchmark.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/dat.gui": "^0.7.7",
    "@types/jest": "^29.1.2",
    "assemblyscript": "^0.21.6",
    "jest": "^29.1.2",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.21.6",
    "@types/stats.js": "^0.17.0",
    "@types/three": "^0.144.0",
    "dat.gui": "^0.7.9",
    "stats.js": "^0.17.0",
    "three": "^0.145.0",
    "vite-plugin-string": "^1.1.2"
  },
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
